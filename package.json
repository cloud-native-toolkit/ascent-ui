{
  "name": "ascent-ui-new",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "express-session": "^1.17.2",
    "http-proxy-middleware": "^2.0.6",
    "ibmcloud-appid": "^6.2.5",
    "marked": "^4.0.15",
    "passport": "^0.5.2",
    "passport-oauth": "^1.0.0",
    "react-inject-env": "^2.1.0",
    "yaml": "^2.0.1"
  },
  "scripts": {
    "start:dev": "dotenv craco start",
    "prestart": "react-inject-env set",
    "start": "node server/server.js",
    "build": "craco --max_old_space_size=4096 build",
    "test": "craco --max_old_space_size=4096 test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown|vfile|unist-util-stringify-position|unified|bail|is-plain-obj|trough|remark-parse|mdast-util-from-markdown|mdast-util-to-string|micromark|decode-named-character-reference|character-entities|remark-rehype|mdast-util|unist-builder|unist-util|property-information|hast-util|.*-separated-tokens)"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@carbon/icons-react": "^10.49.0",
    "@craco/craco": "^7.0.0-alpha.3",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "brace": "^0.11.1",
    "carbon-addons-iot-react": "^2.148.0",
    "carbon-components": "^10.56.0",
    "carbon-components-react": "^7.56.0",
    "carbon-icons": "^7.0.7",
    "d3": "^7.4.4",
    "dotenv-cli": "^6.0.0",
    "react": "^17.0.2",
    "react-ace": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-ga4": "^1.4.1",
    "react-markdown": "^8.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "react-sliding-pane": "^7.1.0",
    "sass": "^1.49.9",
    "superagent": "^7.1.3",
    "web-vitals": "^2.1.4"
  }
}
